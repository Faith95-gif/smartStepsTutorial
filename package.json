{
  "name": "video-call-fullstack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm --prefix backend run dev\" \"npm --prefix frontend run dev\"",
    "build": "npm run build:frontend",
    "build:frontend": "npm --prefix frontend run build",
    "build:backend": "npm --prefix backend run build",
    "cap:copy": "npx cap copy",
    "cap:sync": "npx cap sync",
    "android:clean": "cd android && .\\gradlew.bat clean",
    "android:debug": "npm run build:frontend && npm run cap:sync && cd android && .\\gradlew.bat clean && .\\gradlew.bat assembleDebug",
    "android:release": "npm run build:frontend && npm run cap:sync && cd android && .\\gradlew.bat clean && .\\gradlew.bat assembleRelease",
    "android:install": "npm run build:frontend && npm run cap:sync && cd android && .\\gradlew.bat clean && .\\gradlew.bat installDebug",
    "android:open": "npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@fingerprintjs/fingerprintjs": "^5.0.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.90.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.0",
    "fabric": "^7.0.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.12.0",
    "socket.io-client": "^4.8.1",
    "supabase": "^2.72.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
